# SECURITY NOTICE: Never commit actual .env files with real API keys!

# Supabase Configuration (PRODUCTION)
VITE_SUPABASE_URL=your-production-supabase-url
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# Stripe PRODUCTION Configuration
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
# NOTE: Secret key should ONLY be set on your production server, NEVER in frontend env files
# STRIPE_SECRET_KEY should be configured directly on your hosting platform (Vercel, Netlify, etc.)

# Environment
NODE_ENV=production
VITE_APP_BASE_URL=https://cureliah.com
VITE_API_URL=https://cureliah.com/api

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_MESSAGING=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_DOCUMENTS=true
VITE_ENABLE_SMART_SCHEDULING=true
VITE_ENABLE_MOBILE_APP=true
